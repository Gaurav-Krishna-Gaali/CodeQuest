{
  "compilerOptions": {
    "target": "es2016",
    "module": "Node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": false,
    "jsx": "preserve",
    "moduleResolution": "node16",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
